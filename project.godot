"class": "EquationParser",
"language": "GDScript",
"path": "res://parsers/equation.gd"
}, {
"base": "Resource",
"class": "SaveGame",
"language": "GDScript",
"path": "res://savegame.gd"
}, {
"base": "Area2D",
"class": "add_equation",
"language": "GDScript",
"path": "res://addition_to_equation.gd"
}, {
"base": "Node2D",
"class": "fraction",
"language": "GDScript",
"path": "res://fraction.gd"
}, {
"base": "Control",
"class": "shop_item_button",
"language": "GDScript",
"path": "res://shop/shop_item.gd"
}, {
"base": "Area2D",
